{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "lib": [
      "ESNext"
    ],
    "baseUrl": "./",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@cloudflare"
    ],
    "types": [
      "node"
    ],
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true
  },
  "include": [
    "./**/*.ts",
    "./worker-configuration.d.ts",
    "./env.d.ts"
  ],
  "tsc-alias": {
    "resolveFullPaths": true
  }
}
